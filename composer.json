{
    "name": "kenokokoro/v9-dal",
    "description": "DAL structure for Laravel/Lumen",
    "keywords": [
        "framework",
        "laravel",
        "lumen",
        "data access layer",
        "dal",
        "repository"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.1.3",
        "illuminate/database": "^6.1",
        "illuminate/support": "^6.1",
        "ramsey/uuid": "^3.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "V9\\DAL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "V9\\Tests\\DAL\\": "tests/"
        }
    }
}
